{
  "expo": {
    "name": "SAMP Architect Admin",
    "slug": "samp-architect-admin",
    "version": "1.0.0",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.samparchitect.admin"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#050811"
      },
      "package": "com.samparchitect.admin"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-camera",
      "expo-secure-store"
    ],
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#050811"
  }
}
